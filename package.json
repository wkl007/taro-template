{
  "name": "taro-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro小程序模板",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "taro build --type weapp",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev": "npm run build:weapp -- --watch",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --fix **/*.js",
    "lint:style": "stylelint --fix src/**/*.{css,less,scss,sass}",
    "test": "taro build --type weapp"
  },
  "author": "wkl",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,less,scss,sass}": [
      "stylelint --fix src/**/*.{css,less,scss,sass}",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@tarojs/async-await": "1.3.8",
    "@tarojs/components": "1.3.8",
    "@tarojs/redux": "1.3.8",
    "@tarojs/redux-h5": "1.3.8",
    "@tarojs/router": "1.3.8",
    "@tarojs/taro": "1.3.8",
    "@tarojs/taro-alipay": "1.3.8",
    "@tarojs/taro-h5": "1.3.8",
    "@tarojs/taro-swan": "1.3.8",
    "@tarojs/taro-tt": "1.3.8",
    "@tarojs/taro-weapp": "1.3.8",
    "dayjs": "^1.8.14",
    "nerv-devtools": "^1.4.1",
    "nervjs": "^1.4.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "taro-ui": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@tarojs/plugin-babel": "1.3.8",
    "@tarojs/plugin-csso": "1.3.8",
    "@tarojs/plugin-sass": "1.3.8",
    "@tarojs/plugin-uglifyjs": "1.3.8",
    "@tarojs/webpack-runner": "1.3.8",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.8",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.8",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "redux-logger": "^3.0.6",
    "stylelint": "^10.1.0",
    "stylelint-config-taro-rn": "1.3.8",
    "stylelint-taro-rn": "1.3.8"
  }
}
